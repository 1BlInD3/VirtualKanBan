<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/black">

    <TextView
        android:id="@+id/kontenerbenCikkText"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="1dp"
        android:text="Cikk"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/kontenerbenMegj1Text"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Megj1"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/kontenerbenCikkText"
        app:layout_constraintTop_toTopOf="@+id/kontenerbenCikkText" />

    <TextView
        android:id="@+id/kontenerbenMegj2Text"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Megj2"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/kontenerbenMegj1Text"
        app:layout_constraintTop_toTopOf="@+id/kontenerbenCikkText" />

    <TextView
        android:id="@+id/kontenerbenIntRemText"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Intrem"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/kontenerbenMegj2Text"
        app:layout_constraintTop_toTopOf="@+id/kontenerbenCikkText" />

    <TextView
        android:id="@+id/kontenerbenIgenyeltText"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Igeny"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/kontenerbenIntRemText"
        app:layout_constraintTop_toTopOf="@+id/kontenerbenCikkText" />

    <TextView
        android:id="@+id/konterbenMozgasText"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:text="Mozg"
        android:paddingStart="5dp"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/kontenerbenIgenyeltText"
        app:layout_constraintTop_toTopOf="@+id/kontenerbenCikkText" />
</androidx.constraintlayout.widget.ConstraintLayout>