<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/black">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView7"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="2dp"
        app:cardCornerRadius="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/cikkszamHeader"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:maxLines="1"
            android:textAlignment="textEnd"
            android:textColor="@android:color/black"
            android:paddingStart="10dp"
            android:paddingEnd="5dp"
            android:textSize="16sp"
            tools:text="ELSO SOR" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView8"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        app:cardCornerRadius="1dp"
        app:layout_constraintStart_toEndOf="@+id/cardView7"
        app:layout_constraintTop_toTopOf="@+id/cardView7">

        <TextView
            android:id="@+id/polcText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:textAlignment="textStart"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            tools:text="ELSO SOR" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView9"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        app:cardCornerRadius="2dp"
        app:layout_constraintStart_toEndOf="@+id/cardView13"
        app:layout_constraintTop_toTopOf="@+id/cardView8">

        <TextView
            android:id="@+id/desc1Header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="textStart"
            android:textColor="@android:color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView10"
        android:layout_width="180dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        app:cardCornerRadius="2dp"
        app:layout_constraintStart_toEndOf="@+id/cardView9"
        app:layout_constraintTop_toTopOf="@+id/cardView9">

        <TextView
            android:id="@+id/megnevezes2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:maxLines="1"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="textStart"
            android:textColor="@android:color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView11"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        app:cardCornerRadius="2dp"
        app:layout_constraintStart_toEndOf="@+id/cardView10"
        app:layout_constraintTop_toTopOf="@+id/cardView10">

        <TextView
            android:id="@+id/mennyisegHeader"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:maxLines="1"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="textStart"
            android:textColor="@android:color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView12"
        android:layout_width="100dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:cardCornerRadius="2dp"
        app:layout_constraintStart_toEndOf="@+id/cardView11"
        app:layout_constraintTop_toTopOf="@+id/cardView10">

        <TextView
            android:id="@+id/megjegyzesHeader"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:maxLines="1"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="textStart"
            android:textColor="@android:color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView13"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        app:cardCornerRadius="2dp"
        app:layout_constraintStart_toEndOf="@+id/cardView8"
        app:layout_constraintTop_toTopOf="@+id/cardView10">

        <TextView
            android:id="@+id/cikkszam"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:maxLines="1"
            android:paddingEnd="5dp"
            android:paddingStart="10dp"
            android:textAlignment="textStart"
            android:textColor="@android:color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>